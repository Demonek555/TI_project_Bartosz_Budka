@model IEnumerable<Category>

<nav class="navbar navbar-expand-sm bg-dark navbar-dark">
    <div class="container-fluid">
        <ul class="navbar-nav">
            @foreach (var category in Model)
            {
                <li class="nav-item">
                    <a class="nav-link active" href="@Url.Action("FilmsList","Films", new {categoryName=@category.Name})">@category.Name</a>
                </li>
            }
            <li>
                <a class="btn btn-danger" href="@Url.Action("AllFilms", "Films")">Wszystkie</a>
            </li>
        </ul>
    </div>
</nav>